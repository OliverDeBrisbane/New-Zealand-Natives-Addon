{
   "format_version":"1.10.0",
   "minecraft:client_entity":{
      "description":{
         "identifier":"ccl:kea",
         "materials":{
            "default":"kea"
         },
         "textures":{
            "default":"textures/entity/kea/kea",
            "baby":"textures/entity/kea/kea_baby"
         },
         "geometry":{
            "default":"geometry.kea.walk",
            "flying":"geometry.kea.fly"
         },
         "animations":{
            "move":"animation.bird.move",
            "fly":"animation.phantom.move",
            "general":"animation.bird.general",
            "look_at_target":"animation.common.look_at_target",
            "baby_transform":"animation.bird.baby_transform"
         },
         "scripts":{
            "pre_animation":[
               "variable.wingRotZ = 32.0 * Math.sin((3 + query.life_time * 20.0) * 7.448454);",
               "variable.tailRotX = -5.0 * Math.sin((3 + query.life_time * 20.0) * 14.896908) - 5.0;"
            ],
            "animate":[
               "general",
               {
                  "move":"query.modified_move_speed && query.is_on_ground"
               },
               {
                  "fly":"!query.is_on_ground"
               },
               "look_at_target",
               {
                  "baby_transform":"query.is_baby"
               }
            ]
         },
         "render_controllers":[
            "controller.render.kea"
         ],
         "spawn_egg":{
            "base_color":"#629350",
            "overlay_color":"#F9961B"
         }
      }
   }
}
